<?xml version="1.0" encoding="UTF-8"?>
<fozone-validation>
	<group name="createDictionary">
		<field name="dictCode">
			<rule name="required" message="字典编号不能为空" />
		</field>
		<field name="dictName">
			<rule name="required" message="字典名称不能为空" />
		</field>
	</group>

	<group name="updateDictionary">
		<field name="dictCode">
			<rule name="required" message="字典编号不能为空" />
		</field>
		<field name="dictName">
			<rule name="required" message="字典名称不能为空" />
		</field>
	</group>

	<group name="createEmployee">
		<field name="departmentCode">
			<rule name="required" message="部门编号不能为空"/>
		</field>
		<field name="employeeCode">
			<rule name="required" message="员工编号不能为空"/>
		</field>
	</group>

	<group name="updateEmployee">
		<field name="departmentCode">
			<rule name="required" message="部门编号不能为空"/>
		</field>
		<field name="employeeCode">
			<rule name="required" message="员工编号不能为空"/>
		</field>
	</group>

	<group name="createModule">
		<field name="agencyCode">
			<rule name="required" message="机构编码不能为空"/>
		</field>
		<field name="moduleType">
			<rule name="required" message="业务模块类型不能为空"/>
		</field>
		<field name="moduleCode">
			<rule name="required" message="业务模块编号不能为空"/>
		</field>
		<field name="parentCode">
			<rule name="required" message="父业务模块编码不能为空"/>
		</field>
		<field name="childContentType">
			<rule name="required" message="模块下存放内容类型不能为空"/>
		</field>
		<field name="moduleName">
			<rule name="required" message="业务模块名称不能为空"/>
		</field>
		<field name="orderNum">
			<rule name="required" message="业务模块排序不能为空"/>
		</field>
		<field name="levelNum">
			<rule name="required" message="业务模块级别不能为空"/>
		</field>
		<field name="isAgencyTemplate">
			<rule name="required" message="是否为商家模板不能为空"/>
		</field>
	</group>

	<group name="createModuleOperation">
		<field name="agencyCode">
			<rule name="required" message="机构编码不能为空"/>
		</field>
		<field name="moduleCode">
			<rule name="required" message="模块编码不能为空"/>
		</field>
		<field name="buttonCode">
			<rule name="required" message="按钮编码不能为空"/>
		</field>
		<field name="buttonStyle">
			<rule name="required" message="按钮样式不能为空"/>
		</field>
		<field name="buttonType">
			<rule name="required" message="按钮类型不能为空"/>
		</field>
		<field name="buttonName">
			<rule name="required" message="按钮名称不能为空"/>
		</field>
	</group>

	<group name="updateModuleOperation">
		<field name="buttonStyle">
			<rule name="required" message="按钮样式不能为空"/>
		</field>
		<field name="buttonType">
			<rule name="required" message="按钮类型不能为空"/>
		</field>
		<field name="buttonName">
			<rule name="required" message="按钮名称不能为空"/>
		</field>
	</group>

	<group name="createResource">
		<field name="resourceCode">
			<rule name="required" message="资源编码不能为空"/>
		</field>
		<field name="resourceName">
			<rule name="required" message="资源名称不能为空"/>
		</field>
		<field name="resType">
			<rule name="required" message="资源类型不能为空"/>
		</field>
		<field name="funcId">
			<rule name="required" message="fid不能为空"/>
		</field>
	</group>
</fozone-validation>